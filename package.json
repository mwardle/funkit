{
  "name": "lambdash",
  "version": "0.6.2",
  "description": "Generic functional programming for javascript",
  "main": "src/lambdash.js",
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/mwardle/lambdash.git",
  "scripts": {
    "test": "node_modules/.bin/mocha spec/**/*.spec.js",
    "coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text mocha spec/**/*.spec.js",
    "toc": "node_modules/.bin/doctoc --github --maxlevel 3 README.md"
  },
  "keywords": [
    "functional",
    "generic",
    "algebraic data types",
    "curry",
    "currying"
  ],
  "author": "Mark Wardle <mwwardle@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "code": "^5.2.0",
    "doctoc": "^1.0.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.4.5",
    "nyc": "^11.6.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  }
}
